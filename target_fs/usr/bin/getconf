#!/bin/sh

for i in "$@"
do
    if [[ $(jq -r ".overRides | any(inside(\"$i\"))"  /etc/skylark_config.json) == "true" -a -e /mnt/conf/etc/skylark_config.json ]]
    then
        echo "${i//./_}"=\"$(jq -r ".$i" /mnt/conf/etc/skylark_config.json)\"
    else
        echo "${i//./_}"=\"$(jq -r ".$i"  /etc/skylark_config.json)\"
    fi
done

